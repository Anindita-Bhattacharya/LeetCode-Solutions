class Solution(object):
    def sortArrayByParity(self, A):
        """
        :type A: List[int]
        :rtype: List[int]
        """
        a=[]
        b=[]
        for i in range(len(A)):
            if(A[i]%2==0):
                a.append(A[i])
            else:
                b.append(A[i])
        return a+b
